using Printf
function calculatePay()
    print("Hours worked?")
    hours=parse(Float64,readline())
    print("Rate of pay?")
    rate=parse(Float64,readline())
    if hours<=40
regPay=hours*rate
ovtPay=0
   else
regPay=40*rate
ovtPay(hours40)*rate*1.5
    end
grossPay=regpay+ovtPay
   @printf("\nRegular pay:\$%0.2f\n",regpay)
   @prinf("\nOvertime pay:\$%03.2f\n",ovtpay)
   @printf("Gross pay:\$%0.2f\n",grossPay)
end
calculatePay()

